{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "dist",
    "strictNullChecks": true,
    "esModuleInterop": true,
    "incremental": true,

  },
  "include": ["src/**/*"]
}
