{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "forceConsistentCasingInFileNames": false,
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "dist",
    "strictNullChecks": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "resolveJsonModule": true,
    
    
    // "incremental": true,
  },
  "include": ["src/**/*"]
}
